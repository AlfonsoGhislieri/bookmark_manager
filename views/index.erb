
<h1>Bookmarks</h1>
<form action="/bookmarks" method="post">
  <input type="text" name="bookmark_url">
  <input type="text" name="bookmark_title">  
  <input type="submit" value="Submit">
</form> 
<ul>
  <% @bookmarks.each do |bookmark| %>
    <li>
      <a href="<%= bookmark.url %>">
        <%= bookmark.title %>
      </a>
    </li>
  <% end %>
</ul>

